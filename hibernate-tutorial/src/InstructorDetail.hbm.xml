<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="InstructorDetail" table="Instructor_detail">
      <meta attribute="class-description">
         This class contains the instructor details information. 
      </meta>
      <id name="id" type="int" column="id">
         <generator class="native"/>
      </id>
      <property name="youtubeChannel" column="youtube_channel" type="string"/>
      <property name="hobby" column="hobby" type="string"/>
   </class>
</hibernate-mapping>